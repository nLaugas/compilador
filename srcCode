__verificando anidamientos
let mut integer _a;_b;*_c,
_b:=1_i,
_a:=1_i,
if(_b=_a){
  print('es igual'),
  loop{
     print('se incrementa _a'),
     _a:=_a+1_i,
	 } until (_a = 3_i)
  }
  else{
  print('es distinto'),
  loop{
     print('se incrementa _b'),
     _b:=_b+1_i,
	 } until (_b = 3_i)
  }
end_if
loop{
print('_a es distinto de _b'),
_b:=_b+1_i,
}
until(_a!=_b)
print('finalizacion de programa'),